{
  "name": "ti-slag",
  "version": "0.0.24",
  "description": "Titanium faker API, Titanium app running on Node.js.",
  "keywords": [
    "appcelerator",
    "titanium",
    "alloy",
    "cli",
    "vm"
  ],
  "main": "index.js",
  "bin": {
    "slag": "bin/slag"
  },
  "preferGlobal": true,
  "author": "Kosuke Isobe",
  "homepage": "https://github.com/k0sukey/ti-slag",
  "repository": {
    "type": "git",
    "url": "git://github.com/k0sukey/ti-slag.git"
  },
  "bugs": {
    "url": "https://github.com/k0sukey/ti-slag/issues"
  },
  "scripts": {
    "test": "mocha test.js",
    "covertest": "mocha coverage.js",
    "coverage": "nyc npm test && nyc report",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "config": {
    "nyc": {
      "exclude": [
        "test.js",
        "node_modules/",
        "lib/titanium/4.1.0.GA/",
        "lib/titanium/4.0.0.GA/",
        "lib/titanium/3.5.1.GA/"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.0.0",
    "istanbul": "^0.3.17",
    "lodash": "^3.9.3",
    "meow": "^3.3.0",
    "tiapp.xml": "^0.2.2"
  },
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "acorn": "^2.0.4",
    "coveralls": "^2.11.2",
    "escodegen": "^1.6.1",
    "mocha": "^2.2.5",
    "nyc": "^3.0.0",
    "signal-exit": "^2.1.2"
  }
}
